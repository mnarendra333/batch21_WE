<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping>


	
	<class name="com.pragim.model.Product" table="product_table">
	
		<cache usage="read-only"/>
		<id name="proId" column="pro_id">
			<generator class="sequence">
				<param name="sequence">pro001_seq</param>
			</generator>
		</id>
		<property name="proName" column="pro_name" length="30"/>
		<property name="price"/>
	
	
	
	</class>



</hibernate-mapping>