			Statement stmt1 = connection.createStatement();
			stmt1.executeUpdate("insert into ")
			
			Statement stmt2 = connection.createStatement();
			
			
			Statement stmt3 = connection.createStatement();
			
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		

	}

}
